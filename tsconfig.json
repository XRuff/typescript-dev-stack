{
    "compilerOptions": {
        "target": "es6",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "experimentalDecorators": true,
        "removeComments": true,
        "newLine": "LF",
        "outDir": ".tmp/built"
    },
    "awesomeTypescriptLoaderOptions": {
        "useCache": true,
        "useBabel": true,
        "babelOptions": {
            "presets": ["es2015"]
        }
    },
    "exclude": ["node_modules", "typings/browser", "typings/main", "typings/browser.d.ts"]
}
